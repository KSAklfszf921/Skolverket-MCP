    "sweden",
    "curriculum",
    "skolenheter",
    "school-units",
    "vuxenutbildning",
    "adult-education",
    "yrkeshögskola",
    "komvux",
    "sfi",
    "model-context-protocol",
    "anthropic",
    "claude",
    "chatgpt",
    "ai",
    "llm"
  ],
  "author": {
    "name": "Isak Skogstad",
    "email": "isak.skogstad@me.com",
    "url": "https://x.com/isakskogstad"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KSAklfszf921/Skolverket-MCP.git"
  },
